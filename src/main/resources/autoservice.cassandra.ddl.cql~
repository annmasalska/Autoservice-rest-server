CREATE TABLE auction (
queryType VARCHAR,
carBrand VARCHAR,
params map<varchar, varchar>,  -- {"sort": "aphabeticaly","groupBy": "title"}
model VARCHAR ,
exactCloseDate VARCHAR, 
lot VARCHAR, 
price VARCHAR ,
status VARCHAR, 
timeToEnd map<varchar, varchar>,
details map<varchar, varchar>,
data text,
sorted varchar,
PRIMARY KEY((queryType,carBrand), model, exactCloseDate)
);


CREATE TABLE catalog
(
queryType VARCHAR, -- possible types: currentPartsByPartType, currentPartsByAuto, currentPartsByModel
partType VARCHAR, -- possible types: body part, engine, wheel & tire
carBrand VARCHAR,
model VARCHAR,
title VARCHAR, -- example: door, roof etc
params map<varchar, varchar>, -- {"sort": "aphabeticaly","groupBy": "title"}
price VARCHAR ,
status VARCHAR,
details map<varchar, varchar>,
PRIMARY KEY((queryType, partType), carBrand, model)
);


